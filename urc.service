[Unit]
Description=Simple URC Relay Chat Daemon
Wants=network.target
After=network.target

[Service]
Type=simple
ProtectHome=true
ProtectSystem=true
User=nobody
ExecStart=/opt/urc/urc.py --remote-hub=ntwrkhhtqv73vwd4.onion --socks-port=9050 --hub=127.0.0.1
Restart=always

[Install]
WantedBy=multi-user.target
